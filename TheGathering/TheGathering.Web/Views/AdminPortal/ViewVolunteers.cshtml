@model  TheGathering.Web.Models.VolunteerEventViewModel
@{
    ViewBag.Title = "ViewVolunteers";
}

<h2>Volunteers Signed Up</h2>


@foreach (var item in Model.SignedUpVolunteers)

{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Birthday)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.InterestInLeadership)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.SignUpForNewsLetter)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id })
            @Html.ActionLink("Details", "Details", new { id = item.Id })
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>

}